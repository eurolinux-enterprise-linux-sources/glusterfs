---
 glusterfs.spec.in |    3 +++
 1 files changed, 3 insertions(+), 0 deletions(-)

diff --git a/glusterfs.spec.in b/glusterfs.spec.in
index 53f2007..afbd8ae 100644
--- a/glusterfs.spec.in
+++ b/glusterfs.spec.in
@@ -1070,6 +1070,9 @@ fi
 %if 0%{?_tmpfilesdir:1}
